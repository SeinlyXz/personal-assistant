{
  "name": "bunshint-agent",
  "version": "4.1.0",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run --bun src/main.ts",
    "dev": "bun run --bun --watch src/main.ts",
    "wa:logout": "rm -rf .hiddens/personal-assistant",
    "sessions": "bun run --bun src/main.ts --list",
    "interactive": "bun run --bun src/main.ts --interactive",
    "create": "bun run --bun src/main.ts --interactive",
    "delete": "bun run --bun src/main.ts --interactive"
  },
  "devDependencies": {
    "@types/bun": "^1.2.18",
    "@types/minimist": "^1.2.5"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "baileys": "^6.7.19",
    "baileys-decorators": "https://github.com/SeinlyXz/baileys-decorators",
    "cheerio": "^1.1.0",
    "croner": "^9.1.0",
    "libphonenumber-js": "^1.12.9",
    "libsignal": "^2.0.1",
    "minimist": "^1.2.8",
    "node-cache": "^5.1.2",
    "pino-pretty": "^13.0.0",
    "qrcode-terminal": "^0.12.0",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.3",
    "wa-sticker-formatter": "^4.4.4"
  },
  "trustedDependencies": [
    "@whiskeysockets/baileys",
    "baileys-decorators",
    "esbuild",
    "protobufjs",
    "sharp"
  ]
}